<table><tdody>
  {% for material in materials %}
    <tr>
      <td><ul><li></li></ul></td>
      <td>
        <a title="{% trans %}Edit file of multimedia object{% endtrans %} {{ material.id }}"
           data-toggle="modal" data-dismiss="modal" data-target="#myModal"
           href="{{ path('pumukitnewadmin_material_update', {'id': material.id, 'mmId': mmId}) }}">
          <i class="mdi-editor-mode-edit"></i>
        </a>
      <td>
      <td>
        <a href="#" onclick="window.submit_delete_material('{{ material.id }}', '{{ mmId }}'); return false;">
          <i class="mdi-content-clear"></i>
        </a>
      </td>
      <td>
        <a target="_blank" href="{{ material.getUrl(true) }}">
             <i class="mdi-file-file-download"></i>
        </a>
      </td>
      <td>
        {% if loop.first %}
	  &nbsp;
	{% else %}
	  <a href="#" onclick="$('#materials_mms').load('{{ path('pumukitnewadmin_material_up', {'id': material.id, 'mmId': mmId}) }}', {'_method': 'POST'}); return false;">&#8593;</a>
	{% endif %}
      </td>
      <td>
	{% if loop.last %}
          &nbsp;
	{% else %}
	  <a href="#" onclick="$('#materials_mms').load('{{ path('pumukitnewadmin_material_down', {'id': material.id, 'mmId': mmId}) }}', {'_method': 'POST'}); return false;">&#8595;</a>
	{% endif %}
      </td>
      <td>
        &nbsp; {{ material.id }} - {{ material.getName() }} {% if material.getHide() %}({% trans %}Hidden{% endtrans %}){% endif %}
      </td>
    </tr>
  {% endfor %}
  <tr>
    <td><ul><li></li></ul></td>
    <td colspan="6"><a title="{% trans %}Create Material{% endtrans %}" data-toggle="modal" data-target="#myModal" href="{{ path('pumukitnewadmin_material_create', {'id': mmId}) }}">{% trans %}create{% endtrans %}...</a></td>
  </tr>
</tbody></table>
