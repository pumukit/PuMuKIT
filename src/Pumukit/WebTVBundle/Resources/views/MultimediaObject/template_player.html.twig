<script>
    function resizeFrame(elem) {
        var width = $(elem).width();
        var height = width / 16 * 9;
        $(elem).height(height);
    }

    $(window).resize(function () {
        resizeFrame($('#paellaiframe'));
    });

    $(window).on("load", function() {
        $("#paellaiframe").focus();
    });
</script>

