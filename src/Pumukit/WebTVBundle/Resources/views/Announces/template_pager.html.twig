<div class="row">
    <div class="text-center">
        <h2>
            {{ date | format_datetime(pattern="MMMM-Y",locale=app.request.getLocale())|capitalize }}
        </h2>
    </div>
    <div class="announces">
        {% for object in last %}
            {% if object.isCollection() %}
                {% include '@PumukitWebTV/Resources/template_series.html.twig' with {'show_info': true } %}
            {% else %}
                {% include "@PumukitWebTV/Resources/template_video.html.twig" with {'show_info': true } %}
            {% endif %}
            {% include "@PumukitWebTV/Resources/template_clearfix.html.twig" %}
        {% endfor %}
    </div>
</div>
