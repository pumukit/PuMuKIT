<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="pumukitcore.twig_loader.webtv_bundle" class="Pumukit\CoreBundle\Services\TwigTemplateLoaderService">
            <argument type="service" id="templating.locator" />
            <argument type="service" id="templating.name_parser" />
            <argument>%kernel.project_dir%</argument>
            <argument>PumukitWebTVBundle</argument>
            <argument>PumukitPruebaWebTVBundle</argument>
            <tag name="twig.loader" priority="1"/>
        </service>
    </services>


</container>
