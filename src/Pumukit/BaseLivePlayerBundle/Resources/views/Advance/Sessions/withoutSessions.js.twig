var text = '{{ event.getAlreadyHeldMessage()|default('The event has already been held'|trans)|raw }}';
background = image + style + text + finalText;
$("#live-{{ id }}").html(background);

var date = new Date();
$('#warning').countdown(date.setMinutes(date.getMinutes() + 1)).on('finish.countdown', function () {
    location.reload();
});
