<table><tdody>
  {% if true {# TODO (sfConfig::get('app_transcoder_use')) #} %}
    {% for transcoding in transcoding %}
      <tr>
        <td><ul><li></li><ul></td>
        <td colspan="9">
          {{ transcoding.getStatusText() }}
          {% if (transcoding.getStatusId() == TranscodingPeer::STATUS_ERROR) %}
          <a href="#" onclick="new Ajax.Updater('tracks_mms', '{# TODO path transcoders/retrans?id= $transcoding->getId(). &mm='.$mm.'&preview=true #}', {asynchronous:true, evalScripts:true}); return false;">
            <img alt="{% trans %}retranscode{% endtrans %}" title="{% trans %}retranscode{% endtrans %}" src="/legacy/images/admin/mbuttons/use_inline.gif">
          </a>
          {% endif %}
          {% if (transcoding.getStatusId() == TranscodingPeer::STATUS_ERROR) %}
          <a href="#" onclick="new Ajax.Updater('tracks_mms', '{# TODO path delete transcoders/deletefromfile?id='.$transcoding->getId().'&mm='.$mm.'&preview=true #}, {asynchronous:true, evalScripts:true}); return false;">
            <img alt="{% trans %}delete{% endtrans %}" title="{% trans %}delete{% endtrans %}" src="/legacy/images/admin/mbuttons/delete_inline.gif">
          </a>
          {% endif %}
        </td>
        <td>
          &nbsp;{{ transcoding.getId() }} - <strong>{{ transcoding.getPerfil().getName() }}</strong>
	  - {{ basename(transcoding.getPathini()) }}
          - {{ transcoding.getDurationString() }}
        </td>
      </tr>
    {% endfor %}
  {% endif %}

