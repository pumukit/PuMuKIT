            <div class="trans_button_up"><img src="/legacy/images/admin/mbuttons/frame_inline.gif" alt="{% trans %}frame{% endtrans %}" /></div>
            <div class="trans_button_info">{% trans %}Capture frame from{% endtrans %}:</div>
          </div>
        
          <div class="list_options">
            <ul style="">
              <li><a href="#" onclick="new Ajax.Updater('pic_mms', {# TODO 'files/pic?id='.$file->getId().'&mm='.$file->getMmId().'&preview=true' #}, {asynchronous:true, evalScripts:true, onLoading:function(request, json){$('pic_mms_load').show()}});; return false;">{% trans %}Auto{% endtrans %}</a></li>
              <li><a href="#" onclick="new Ajax.Updater('pic_mms', '{# TODO 'files/pic?id='.$file->getId().'&mm='.$file->getMmId().'&preview=true&numframe=10%' #}', {asynchronous:true, evalScripts:true, onLoading:function(request, json){$('pic_mms_load').show()}}); return false;">{% trans %}10%{% endtrans %}</a></li>
              <li><a href="#" onclick="new Ajax.Updater('pic_mms', '{# TODO 'files/pic?id='.$file->getId().'&mm='.$file->getMmId().'&preview=true&numframe=25%' #}', {asynchronous:true, evalScripts:true, onLoading:function(request, json){$('pic_mms_load').show()}}); return false;">{% trans %}25%{% endtrans %}</a></li>
              <li><a href="#" onclick="new Ajax.Updater('pic_mms', '{# TODO 'files/pic?id='.$file->getId().'&mm='.$file->getMmId().'&preview=true&numframe=50%' #}', {asynchronous:true, evalScripts:true, onLoading:function(request, json){$('pic_mms_load').show()}}); return false;">{% trans %}50%{% endtrans %}</a></li>
