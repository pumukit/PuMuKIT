              <li><a href="#" onclick="new Ajax.Updater('pic_mms', '{# TODO 'files/pic?id='.$file->getId().'&mm='.$file->getMmId().'&preview=true&numframe=90%' #}', {asynchronous:true, evalScripts:true, onLoading:function(request, json){$('pic_mms_load').show()}}); return false;">{% trans %}90%{% endtrans %}</a></li>
              <li class="cancel"><a href="#" onclick="return false;">{% trans %}Cancel{% endtrans %}...</a></li>
            </ul>
        
        
          </div>
        </div>
        </span>
      </td>
      {% endif %}
      
      {% if true {# TODO (sfConfig::get('app_videoserv_browser')) #} %}
         <td><a target="_blank" href="{# TODO 'files/download?id='.$file->getId() #}"><img alt="{% trans %}download{% endtrans %}" title="{% trans %}download{% endtrans %}" src="/legacy/images/admin/mbuttons/download_inline.gif"></a></td>
      {% endif %}     

      {% if true {# TODO (track.getPerfil().getDisplay()) #} %}
      <td>
        <span class="trans_button" onclick="$('list_perfiles_{{ track.id }}').toggle()"><img src="/legacy/images/admin/mbuttons/use_inline.gif" alt="X" />
        <div class="trans_menu" id="list_perfiles_{{ track.id }}" style="display:none">
        
          <div class="mas_info" style="">
            <div class="trans_button_up"><img src="/legacy/images/admin/mbuttons/use_inline.gif" alt="X" /></div>
            <div class="trans_button_info">{% trans %}Transcode to profile{% endtrans %}:</div>
          </div>
        
          <div class="list_options">
            <ul style="">
              {% set profile = '' %}
              {% for per in profile {# TODO (PerfilPeer::doSelectToWizard(false) as $per) #} %}
                <li><a href="#" onclick="new Ajax.Updater('tracks_mms', '{# TODO 'files/retrans?id='.$file->getId().'&mm='.$file->getMmId().'&profile='.$per->getId() #}, {asynchronous:true, evalScripts:true}); return false;">{{ per.getName() }}</a></li>
              {% endfor %}
              <li class="cancel"><a href="#" onclick="return false;">{% trans %}Cancel{% endtrans %}...</a></li>
            </ul>        
          </div>

        </div>
        </span>
      </td>

      {% else %}

      <td>        
        <a title="{% trans %}play{% endtrans %}" href="#" onclick="Shadowbox.open({
            title:      '{% trans %}Preview{% endtrans %}',
            content:    '{{ track.getUrl() }}',
            type:       'flv',
            height:     480,
            width:      640
          }); return false;">
          <img alt="{% trans %}play{% endtrans %}" title="{% trans %}play{% endtrans %}" src="/legacy/images/admin/mbuttons/play_inline.gif">
        </a>
      </td>
      {% endif %}
      <td>
      {% if loop.first %}
        &nbsp;
      {% else %}
        <a href="#" onclick="new Ajax.Updater('tracks_mms', '{# TODO 'files/up?id='.$file->getId().'&mm='.$mm.'&preview=true' #}', {asynchronous:true, evalScripts:true}); return false;">&#8593;</a>
      {% endif %}
      </td>
      <td>
      {% if loop.last %}
        &nbsp;
      {% else %}
        <a href="#" onclick="new Ajax.Updater('tracks_mms', '{# TODO 'files/down?id='.$file->getId().'&mm='.$mm.'&preview=true' #}', {asynchronous:true, evalScripts:true}); return false;">&#8595;</a>
      {% endif %}
      </td>

      <td>
        &nbsp;{{ track.id }} - <strong>TODO {# {{ track.getPerfil().getName() }} #}</strong>
        {{ track.getDescription() }}
        (TODO {# {{ basename(track.getFile()) }}/{{ track.getLanguage().getName() }} #})
